<h1>Listing group_shots</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @group_shots.each do |group_shot| %>
      <tr>
        <td><%= link_to 'Show', group_shot %></td>
        <td><%= link_to 'Edit', edit_group_shot_path(group_shot) %></td>
        <td><%= link_to 'Destroy', group_shot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Group shot', new_group_shot_path %>
